{
  "bindings": [
    {
      "name": "vigoTimer",
      "type": "timerTrigger",
      "schedule": "0 */1 * * * *",
      "direction": "in"
    },
    {
      "name": "vigoQueue",
      "type": "serviceBus",
      "queueName": "pullQueue",
      "connection": "SERVICE_BUS_CONNECTION",
      "direction": "out"
    },
    {
      "name": "vigoBlob",
      "type": "blob",
      "path": "outcontainer/{name}.pdf",
      "connection": "STORAGE_CONNECTION",
      "direction": "out"
    }
  ],
  "disabled": false
}
