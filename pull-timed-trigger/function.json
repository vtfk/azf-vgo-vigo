{
  "bindings": [
    {
      "name": "vigoPullTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "name": "vigoPullOutputQueue",
      "type": "serviceBus",
      "queueName": "testqueue",
      "connection": "MyServiceBusConnection",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "vigoPullOutputBlob",
      "path": "outcontainer/{name}.pdf",
      "connection": "STORAGE_CONNECTION",
      "direction": "out"
    }
  ],
  "disabled": false
}
